{
  "name": "homeblebrag",
  "version": "1.0.0",
  "private": true,
  "description": "realtor.com homeblebrag",
  "author": "Josh Mock <josh@805collective.com>",
  "scripts": {
    "deploy": "npm-run-all build:* deploy:*",
    "build:front-end": "rm -rf dist && NODE_ENV=production webpack -p --progress --colors && npm run copy-styles",
    "copy-styles": "rm -rf lambda/video-builder/dist && mkdir -p lambda/video-builder/dist && cp dist/*.{png,jpg,otf} lambda/video-builder/dist && cp dist/style-*.css lambda/video-builder/dist/style.css",
    "deploy:front-end": "node ./scripts/upload-front-end.js",
    "build:lambda-video-builder": "cd ./lambda/video-builder && npm install && npm run zip",
    "build:lambda-receive-post": "cd ./lambda/receive-post && npm install && npm run zip",
    "build:lambda-build-image": "cd ./lambda/build-image && npm install && npm run zip",
    "build:lambda-validate-address": "cd ./lambda/validate-address && npm install && npm run zip",
    "build:lambda-share-email": "cd ./lambda/share-email && npm install && npm run zip",
    "deploy:lambdas": "node ./scripts/deploy-lambdas.js",
    "watch": "NODE_ENV=development webpack-dev-server --inline --hot --content-base dist/",
    "lint": "eslint {src,test}/",
    "test": "mocha --require babel-register --recursive test/ --reporter dot",
    "test-watch": "mocha --require babel-register --recursive test/ --reporter min --watch"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "bluebird": "^3.3.5",
    "classnames": "^2.2.5",
    "fluent-ffmpeg": "^2.0.1",
    "keymirror": "^0.1.1",
    "react-dropdown": "^1.0.4",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "uuid-v4": "^0.1.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.4.1",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-register": "^6.7.2",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "eslint": "^3.6.1",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flux-standard-action": "^0.6.1",
    "html-webpack-plugin": "^2.22.0",
    "lodash": "^4.16.2",
    "mocha": "^2.4.5",
    "node-sass": "^3.10.0",
    "npm-run-all": "^3.0.0",
    "pre-commit": "^1.1.3",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "s3": "^4.4.0",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.1"
  }
}
